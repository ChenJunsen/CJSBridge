<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cjs.cjsbridge">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/net_security_cfg"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".web.CJSWebActivityAdvanced"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".web.CJSWebActivityPrompt"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".web.CJSWebActivitySimple"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".pages.Demo1Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>